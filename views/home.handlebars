<h4>All Posts:</h4>
{{!-- Cards for posts --}}
<div class="row">
{{#each posts}}
<div class="card post-card" style="width: 18rem;">
     <div class="card-header post-title">
    {{title}}
  </div>
  <div class="card-body">
    <p class="card-text">{{contents}}</p>
    <a href="/{{id}}">View Post</a>
  </div>
</div>
{{/each}}
</div>
